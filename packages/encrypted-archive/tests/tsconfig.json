{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Modules */
    "types": [
      "node",
      "jest",
      "jest-extended",
      "@sounisi5011/jest-binary-data-matchers"
    ],
    "resolveJsonModule": true,

    /* JavaScript Support */
    "allowJs": true,

    /* Emit */
    "noEmit": true
  },
  "include": ["./**/*", "../src/**/*", "../package.json"]
}
