{
  "name": "@sounisi5011/file-encryptor",
  "version": "0.0.0",
  "description": "Convert files and data into encrypted binary data",
  "keywords": [
    "AES",
    "AES-256",
    "AES-256-GCM",
    "AES-GCM",
    "AES256GCM",
    "AESGCM",
    "Argon2",
    "Argon2d",
    "ChaCha20",
    "ChaCha20-Poly1305",
    "NodeJS",
    "Poly1305",
    "binary",
    "buffer",
    "cipheriv",
    "crypto",
    "data",
    "decrypt",
    "decryption",
    "encrypt",
    "encryption",
    "file",
    "gcm",
    "password",
    "protect",
    "secure",
    "security",
    "stream",
    "transform",
    "util",
    "utility"
  ],
  "homepage": "https://github.com/sounisi5011/npm-packages/tree/main/packages/file-encryptor#readme",
  "bugs": {
    "url": "https://github.com/sounisi5011/npm-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sounisi5011/npm-packages.git",
    "directory": "packages/file-encryptor"
  },
  "license": "MIT",
  "author": "sounisi5011",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "./src/",
    "example": "./examples/",
    "test": "./tests/"
  },
  "files": [
    "./dist/"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build-protobuf": "run-p build-protobuf:*",
    "build-protobuf:src": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:./src/header/protocol-buffers/ --ts_out=./src/header/protocol-buffers/ --proto_path ./src/header/protocol-buffers/ ./src/header/protocol-buffers/header.proto",
    "build-protobuf:test": "glob-exec --foreach 'tests/unit/fixtures/*.prototxt' -- 'protoc --encode=Header --proto_path ./src/header/protocol-buffers/ ./src/header/protocol-buffers/header.proto < {{file}} > {{file.path.replace(/\\.prototxt$/, `.bin`)}}'",
    "build:copy-js-and-dts": "cpy '**/*.{js,d.ts}' ../dist/ --cwd=./src/ --parents",
    "build:tsc": "tsc",
    "lint:tsc": "run-p lint:tsc:*",
    "lint:tsc:src": "tsc --noEmit",
    "lint:tsc:test": "tsc -p ./tests/ --noEmit",
    "test": "run-if-supported-node run-p test:*",
    "test:examples": "run-s build test:examples:*",
    "test:examples:index": "node ./examples/index.js",
    "test:jest": "jest"
  },
  "dependencies": {
    "@jorgeferrero/stream-to-buffer": "^2.0.6",
    "argon2-browser": "^1.15.4",
    "google-protobuf": "^3.15.8",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@sounisi5011/scripts--run-if-supported-node": "workspace:^0.0.0",
    "@sounisi5011/ts-type-util-has-own-property": "workspace:^1.0.0",
    "@types/argon2-browser": "1.12.0",
    "@types/google-protobuf": "3.15.2",
    "@types/node": "*",
    "@types/varint": "6.0.0",
    "combinate": "1.1.3",
    "cpy-cli": "3.1.1",
    "escape-string-regexp": "4.0.0",
    "glob-exec": "0.1.1",
    "grpc_tools_node_protoc_ts": "5.2.1",
    "jest-extended": "0.11.5",
    "jest-matcher-utils": "26.6.2",
    "multicodec": "3.0.1",
    "type-fest": "1.1.1"
  },
  "engines": {
    "node": "12.x || 14.x || 15.x || 16.x"
  },
  "runkitExampleFilename": "./examples/index.js"
}
