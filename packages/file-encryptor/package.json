{
  "name": "@sounisi5011/file-encryptor",
  "version": "0.0.0",
  "description": "Convert files and data into encrypted binary data",
  "keywords": [
    "AES",
    "AES-256",
    "AES-256-GCM",
    "AES-GCM",
    "AES256GCM",
    "AESGCM",
    "Argon2",
    "Argon2d",
    "ChaCha20",
    "ChaCha20-Poly1305",
    "NodeJS",
    "Poly1305",
    "binary",
    "buffer",
    "cipheriv",
    "crypto",
    "data",
    "decrypt",
    "decryption",
    "encrypt",
    "encryption",
    "file",
    "gcm",
    "password",
    "protect",
    "secure",
    "security",
    "stream",
    "transform",
    "util",
    "utility"
  ],
  "homepage": "https://github.com/sounisi5011/npm-packages/tree/main/packages/file-encryptor#readme",
  "bugs": {
    "url": "https://github.com/sounisi5011/npm-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sounisi5011/npm-packages.git",
    "directory": "packages/file-encryptor"
  },
  "license": "MIT",
  "author": "sounisi5011",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "./src/",
    "example": "./examples/",
    "test": "./tests/"
  },
  "files": [
    "./dist/"
  ],
  "scripts": {
    "build": "tsc",
    "build-flatbuffers": "run-s build-flatbuffers:flatc build-flatbuffers:fix",
    "build-flatbuffers:fix": "globby-exec fix-flatbuffers-generated-ts '{{src/**/flatbuffers/*_generated.ts}}'",
    "build-flatbuffers:flatc": "globby-exec flatc --ts -o ./src/header/flatbuffers/ '{{src/header/flatbuffers/*.fbs}}'",
    "lint:tsc": "run-p lint:tsc:*",
    "lint:tsc:src": "tsc --noEmit",
    "lint:tsc:test": "tsc -p ./tests/ --noEmit",
    "test": "run-p test:*",
    "test:examples": "run-s build test:examples:*",
    "test:examples:index": "node ./examples/index.js",
    "test:jest": "jest"
  },
  "dependencies": {
    "argon2-browser": "^1.15.4",
    "flatbuffers": "^1.12.0",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@sounisi5011/scripts--fix-flatbuffers-generated-ts": "workspace:^0.0.0",
    "@sounisi5011/ts-type-util-has-own-property": "workspace:^1.0.0",
    "@types/argon2-browser": "1.12.0",
    "@types/flatbuffers": "1.10.0",
    "@types/node": "*",
    "@types/varint": "6.0.0",
    "multicodec": "3.0.1"
  },
  "engines": {
    "node": "12.x || 14.x || 15.x"
  },
  "runkitExampleFilename": "./examples/index.js"
}
